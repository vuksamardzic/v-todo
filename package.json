{
  "name": "webpack-node",
  "version": "1.0.0",
  "description": "Node.js w/ webpack. :hammer:",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod && node ./dist/server.js",
    "dev": "./node_modules/.bin/webpack --mode development --watch",
    "prod": "./node_modules/.bin/webpack --mode production",
    "init": "npm i -S body-parser cors express mongoose && npm -D i @babel/core @babel/preset-env babel-core \"babel-loader@^8.0.0-beta\" nodemon-webpack-plugin webpack webpack-cli webpack-node-externals"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuksamardzic/webpack-node.git"
  },
  "author": "Vuk Samardžić <samardzic.vuk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vuksamardzic/webpack-node/issues"
  },
  "homepage": "https://github.com/vuksamardzic/webpack-node#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mongoose": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.4",
    "nodemon-webpack-plugin": "^3.0.1",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
